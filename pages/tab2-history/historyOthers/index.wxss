/* pages/historySelf/index.wxss */
.list-title{
  display : flex;
  justify-content : space-between;
  align-items: center;
  /* width: 100%; */
  height: 100rpx;
  padding: 5rpx 5rpx auto 5rpx;
  background-color: #FFFFFF;
}
.list-item{
  display : flex;
  font-size: 35rpx;
  border-style: dashed;
  border-width: 2rpx;
  border-color: #DDDDDD;
  border: none none dashed none;
  color: #555555;
  /* margin-bottom: 30rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx; */
  padding: 20rpx;
  line-height: 100%;
  justify-content : space-between;
  align-items: flex-start;
 
}

.more {
  width: 30rpx;
  height: 30rpx;
  transform: rotate(90deg);
}
.less {
  width: 30rpx;
  height: 30rpx;
  transform: rotate(270deg);
}

.total-list{
  /* height: 100rpx; */
  margin: 15rpx 20rpx 15rpx 20rpx;
  border-radius: 10rpx;

}
.one-list{
  border-style: solid;
  border-color: #DDDDDD;
  border-width: 2rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  border-top: none;
  background-color: #f5f5f5;
}
page{
  height: 100%;
  background-color: rgb(250, 250, 250);
}